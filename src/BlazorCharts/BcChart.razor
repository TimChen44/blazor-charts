@namespace BlazorCharts
@typeparam TData
@attribute [CascadingTypeParameter(nameof(TData))]

<CascadingValue Value="this">
    <svg width="@Width" height="@Height" xmlns="http://www.w3.org/2000/svg">
        <BcSeriesGroup TData="TData"></BcSeriesGroup>
        <BcAxisGroup TData="TData"></BcAxisGroup>
        @ChildContent
        <BcLegend TData="TData"></BcLegend>
        <BcLabels TData="TData"></BcLabels>
    </svg>
</CascadingValue>

